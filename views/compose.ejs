<%- include("partials/header"); -%>

<h1>Show Notes</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label>Date</label>
        <input class="form-control" type="text" name="postTitle" autocomplete="off">
        <label>Notes</label>
        <textarea class="form-control" name="postBody" rows="5" col="30"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Post</button>
</form>

<%- include("partials/footer"); -%>